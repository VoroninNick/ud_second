    overflow: hidden

.l-search-form-wrap
  .input
    padding-top: 15px
    > label
      font:
        size: rem-calc(14)
      line-height: rem-calc(16.8)
      color: #2E3E51
      margin-bottom: 6px
    select
      width: 100%
      margin: 0
      border: none
    .SumoSelect
      width: 100%
      .optWrapper
        top: 35px
      ul.options
        max-height: none
        height: auto
        overflow: visible
        li
          height: 35px
          border-radius: 0!important
          border-bottom: 1px solid #BEC3C7
          padding: 0 0 0 35px
          &:hover
            background-color: #EDF0F1
          span
            i
              background: #fff
              border: 1px solid #BEC3C7
          label
            font:
              size: rem-calc(14)
            line-height: rem-calc(35)
            color: #BEC3C7
          &.selected
            span
              i
                background-image: url('<%=asset_path("UAD-check-(white).png")%>')
                background-color: #3598DB
                border: none
            label
              color: #3598DB
    .SlectBox
      width: 100%
      padding: 0
      height: 35px
      border: none
      margin: 0
      > span
        height: 100%
        line-height: rem-calc(35)
        color: #BEC3C7
        font:
          size: rem-calc(14)
          weight: 300
        padding-left: 12px
      > label
        background-color: transparent
        > i
          background-image: url('<%=asset_path("UAD-triangle-grey.png")%>')
          width: 100%
          height: 100%
  .input-ud-button
    input[type="submit"]
      width: 100%
      color: #EDF0F1 !important
      font-size: 1.125rem !important
  a
    font-size: 0.75rem
    font-weight: 300
    font-family: "Fira Sans", sans-serif
    color: #D03F4A
    line-height: 0.9rem
    padding-top: 7px
    padding-left: 12px
    span
      color: #2E3E51
      height: auto
      border: none
      display: inline-block
  .l-search-result-wrap
    margin-top: 2px
    padding-top: 15px
    padding-bottom: 65px
    h3
      color: #2E3E51
      font:
        size: rem-calc(20)
        weight: 400
      line-height: rem-calc(25)
      text-transform: uppercase
      margin-bottom: 30px
      span
        color: #D03F4A

