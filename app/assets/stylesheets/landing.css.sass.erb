@import "mixins"


body.landing-page-wrap
  display: block !important
  .l-round-count-wrap
    position: relative
    height: 24px
    .round-count
      +my_horizontal_align
.l-header-wrap
  height: 100px
  .l-header
    height: 100%
    .l-header-login-form-wrap
      position: relative
      width: 100%
      .l-header-login-form-inner
        position: absolute
        left: 0
        right: 0
        .l-header-login-form
          position: relative
          z-index: 1
          background-color: #EDF0F1
          padding-top: 25px
          padding-bottom: 35px
          p
            color: #2E3E51
            font:
              size: rem-calc(10)
              weight: 300
            line-height: rem-calc(12)
            margin-bottom: 9px
          form
            a
              color: #D03F4A
              font:
                size: rem-calc(10)
                weight: 300
              line-height: rem-calc(12)
          .input
            .t-label
              label
                font:
                  size: rem-calc(10)
                  weight: 300
                line-height: rem-calc(12)
                padding-left: 0
            label
              color: #2E3E51
              font-size: 0.75rem
              padding-left: 12px
              line-height: 1.05rem
              margin-bottom: 6px
            input
              color: #D03F4A
          .l-hf-socials-wrap
            a
              display: inline-block
              background-color: #287FB8
              height: 35px
              width: 100%
              font:
                size: rem-calc(12)
              line-height: rem-calc(35)
              color: #EDF0F1
              text-transform: uppercase
              border-left: 1px solid #EDF0F1
              border-right: 1px solid #EDF0F1
              padding-left: 40px
              svg
                width: 30px
                height: 30px
                position: absolute
                left: 6px
                top: 2.5px
                path
                  fill: #fff
          .l-hf-label
            width: 100%
            height: 30px
            overflow: hidden
            margin-top: 5px
            b
              display: inline-block
              background: #EDF0F1
              font:
                family: 'dearjoe_1_msregular'
                size: rem-calc(23)
              +my_horizontal_align
              &:before
                display: block
                content: ''
                width: 26px
                height: 26px
                position: absolute
                top: 0
                left: -5px
                background-color: #EDF0F1
                z-index: -1
              &:after
                display: block
                content: ""
                position: absolute
                top: 13px
                left: -1000px
                width: 2000px
                height: 0px
                border-bottom: 2px dotted #48647C
                z-index: -2
          input[type="submit"]
            width: 100%
            color: #EDF0F1 !important
  ul
    list-style: none
    margin: 0
    height: 100px
    li
      height: 100%
      float: left
      padding: 0 25px
      a
        display: inline-block
        +my_vertical_align
        color: #EDF0F1
        font:
          size: rem-calc(16)
          weight: 400
        height: 19px
        line-height: rem-calc(19)
  .l-logo-wrap
    .l-icon
      width: 40%
      float: left
      height: 100px
      img
        +my_vertical_align
    .l-title
      width: 60%
      float: left
      height: 100px
      img
        +my_vertical_align
  .l-ud-button
    font:
      size: rem-calc(18)
      family: 'Fira Sans', sans-serif
      weight: 400
    color: #EDF0F1
    line-height: rem-calc(35)
    height: rem-calc(35)
    display: inline-block
    text:
      transform: uppercase
      align: center
    background-color: #D03F4A
    text-shadow: 1px 1px 1px #9b3036, 2px 2px 1px #9b2e36, 3px 3px 1px #9d2e37, 4px 4px 1px #9e2f37, 5px 5px 1px #a02f38, 6px 6px 1px #a13038, 7px 7px 1px #a33039, 8px 8px 1px #a53139, 9px 9px 1px #a6313a, 10px 10px 1px #a8323b, 11px 11px 1px #aa323b, 12px 12px 1px #ab333c, 13px 13px 1px #ad333c, 14px 14px 1px #ae343d, 15px 15px 1px #b0343d, 16px 16px 1px #b2353e, 17px 17px 1px #c23c47, 18px 18px 1px #b5363f, 19px 19px 1px #b63640, 20px 20px 1px #b83740, 21px 21px 1px #ba3741, 22px 22px 1px #bb3842
    overflow: hidden
    padding: 0 20px
.bx-wrapper
  position: relative
  .bx-controls
    .bx-controls-direction
      a
        position: absolute
        text-indent: -9999px
        width: 14px
        height: 28px
        //background-color: royalblue
        background-size: 100%
        background-repeat: no-repeat
        bottom: 45%
        &.bx-prev
          left: -20px
          background-image: asset_url("UD-icon-arrow-up-l.svg")
        &.bx-next
          right: -20px
          background-image: asset_url('UD-icon-arrow-up-r.svg')
.l-main-banner-wrap
  height: 650px
  .bx-controls-direction
    display: none
  .bx-pager
    height: 40px
    background: #2E2E36
    width: 100%
    position: relative
    text-align: center
    .bx-pager-item
      display: inline-block
      margin: 10px 30px
      a
        background-image: asset_url('UD-icon-diamond-white.svg')
        background-size: 100%
        background-repeat: no-repeat
        text-indent: -9999px
        display: block
        width: 20px
        &.active
          background-image: asset_url('UD-icon-diamond-red.svg')
  .l-banner-information-about-profile-wrap
    position: absolute
    left: 0
    top: 0
    right: 0
    bottom: 0
    .l-banner-information-about-profile-inner
      width: 100%
    .l-banner-about-profile
      position: relative

      .lb-about-profile-wrap
        position: absolute
        //background-color: yellow
        height: 610px
        left: 0
        right: 0
        .lb-about-profile
          position: absolute
          left: 0
          right: 0
          bottom: 144px
          //background-color: red

  .l-banner-login-form-wrap
    height: 0
    position: absolute
    left: 0
    top: 100px
    right: 0
    .l-banner-login-form-inner
      height: 0
      position: relative
      .l-banner-login-form
        height: 410px
        background-color: #EDF0F1
        width: 90%
        position: absolute
        top: 55px
        right: 0
        z-index: 10
        .lbf-registration-form
          margin-top: 10px

          i
            color: #2E2E36
            font:
              size: rem-calc(25)
              weight: 300
              family: 'dearjoe_1_msregular'
          .ud-gender-input
            position: relative
            margin: 5px 0 20px
            .lbf-gender-label-wrap
              .lbf-ico
                width: 22px
                float: left
                svg
                  height: 20px
                  width: 100%
              .lbf-label
                display: inline-block
                height: 20px
                float: left
                label
                  padding: 0 8px
                  line-height: 20px
                  font-size: 10px
                  text-transform: uppercase
                  margin: 0
                  color: #fff
              &.lbf-male
                .lbf-ico
                  margin-right: 3px
                .lbf-label
                  background-color: #287FB8
                path
                  fill: #287FB8
              &.lbf-female
                .lbf-ico
                  margin-right: 8px
                .lbf-label
                  background-color: #D03F4A
                path
                  fill: #D03F4A
            input
              position: absolute
              left: -1000px
              visibility: hidden
          .input-ud-button
            margin-top: 30px
            input[type="submit"]
              height: 45px
              font:
                size: rem-calc(33) !important
              line-height: rem-calc(45)
              color: #EDF0F1 !important
              width: 100%
          .input
            input
              height: 45px
            label
              color: #2E3E51
              font:
                size: rem-calc(12)
              padding-left: 12px
              line-height: 1.05rem
              margin-bottom: 6px

        .lbf-header-wrap
          height: 60px
          width: 100%
          .lbf-tumbler
            height: 100%
            float: left
            background-color: #D03F4A
            padding-top: 10px
            cursor: pointer
            b, p
              color: #EDF0F1
            b
              font:  
                size: rem-calc(21)
                weight: 400
              line-height: rem-calc(25)
              text-transform: uppercase
            p
              margin-bottom: 0
              font:
                size: rem-calc(10)
                weight: 300
              line-height: rem-calc(12)
            &.lbf-active
              background-color: #EDF0F1
              cursor: default
              b, p
                color: #2E3E51

          .lbf-registration-wrap
            width: 60%
          .lbf-login-wrap
            width: 40%
  .bx-wrapper
    height: 610px
    .bx-viewport
      height: 100%
  ul
    height: 100%
    margin: 0
    list-style: none
    li
      height: 100%
      a
        height: 100%
        .image
          max-width: 100%
          width: 100%
          height: 100%
          overflow: hidden
          -webkit-background-size: cover !important
          -moz-background-size: cover !important
          -o-background-size: cover !important
          background-size: cover !important
          margin: 0 auto

.l-about-project-wrap
  background-color: #EDF0F1
  margin-bottom: 40px

.l-who-is-online-now-wrap
  background-color: #EDF0F1
  .l-header
