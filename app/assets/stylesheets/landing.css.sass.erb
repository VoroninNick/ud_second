.l-men-say-wrap
  margin-top: 35px
  margin-bottom: 45px
  .notification-container
    background-color: #2E3E51
    display: block
    left: 90%
    top: 0
    bottom: 0
    min-width: 100px
    width: 280px
    min-height: auto
    border: none
    &:after
      display: block
      content: ""
      position: absolute
      top: 35px
      left: -11px
      width: 0
      height: 0
      border-style: solid
      border-width: 11px 11px 11px 0
      border-color: transparent #2E3E51 transparent transparent
    &:before
      display: none
  .l-men-say
    padding: 25px
    &:before
      display: inline-block
      content: ''
      position: absolute
      background-image: asset-url("UAD-quote-1.svg")
      background-size: 100%
      background-repeat: no-repeat
      left: 10px
      top: 10px
      width: 57px
      height: 42px
      z-index: -1
    &:after
      display: inline-block
      content: ''
      position: absolute
      background-image: asset-url("UAD-quote-2.svg")
      background-size: 100%
      background-repeat: no-repeat
      right: 15px
      bottom: 15px
      width: 57px
      height: 42px
    p
      color: #D03F4A
      font:
        size: rem-calc(12)
        weight: 300
      line-height: rem-calc(16)
  .lms-one-wrap
    cursor: pointer
    .image
      max-width: 100%
      width: 100%
      height: 0
      padding-bottom: 110%
      overflow: hidden
      -webkit-background-size: cover !important
      -moz-background-size: cover !important
      -o-background-size: cover !important
      background-size: cover !important
      margin: 0 auto
    b
      color: #2E3E51
      font:  
        size: rem-calc(21)
        weight: 400
      line-height: rem-calc(25)
      text-transform: uppercase
      width: 100%
      display: inline-block
      text-align: center

    i
      color: #D03F4A
      font:
        size: rem-calc(25)
        weight: 300
        family: 'dearjoe_1_msregular'
      width: 100%
      display: inline-block
      text-align: center

