.favorites-woman-wrap
  .favorites-nav-wrap
    .row
      .columns
        .favorites-nav
          nav
            li
              a.active
                .icon
                  =embedded_svg "UD-icon-star.svg"
                  .round-count data-count="17"
                .title
                  b My special list

            li
              a
                .icon
                  =embedded_svg "UD-icon-diamond favorites.svg"
                  .round-count data-count="11"
                .title
